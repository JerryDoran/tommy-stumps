---
import { Image } from "astro:assets";
import MainLayout from "../layouts/MainLayout.astro";
import stump1 from '../images/stump-1-min.png'
import stump2 from '../images/stump-2-min.png'
import stump3 from '../images/stump-3-min.png'
import stump4 from '../images/stump-4-min.png'
import stump5 from '../images/stump-5-min.png'
import stump6 from '../images/stump-6-min.png'
import stump7 from '../images/stump-7-min.png'
import stump8 from '../images/stump-8-min.png'
import stump9 from '../images/stump-9-min.png'
import stump10 from '../images/stump-10-min.png'
import stump11 from '../images/stump-11-min.png'
import stump12 from '../images/stump-12-min.png'
